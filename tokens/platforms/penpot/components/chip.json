{
  "chip": {
    "gap": {
      "$value": "{size.pos.xxxsmall}",
      "$type": "dimension"
    },
    "padding": {
      "top": {
        "$value": "{size.null}",
        "$type": "dimension"
      },
      "right": {
        "$value": "{size.pos.xxxsmall}",
        "$type": "dimension"
      },
      "bottom": {
        "$value": "{size.null}",
        "$type": "dimension"
      },
      "left": {
        "$value": "{size.pos.xxxsmall}",
        "$type": "dimension"
      }
    },
    "height": {
      "$value": "{size.pos.xsmall}",
      "$type": "dimension"
    },
    "margin": {
      "top": {
        "$value": "{size.null}",
        "$type": "dimension"
      },
      "right": {
        "$value": "{size.null}",
        "$type": "dimension"
      },
      "bottom": {
        "$value": "{size.null}",
        "$type": "dimension"
      },
      "left": {
        "$value": "{size.pos.xxsmall}",
        "$type": "dimension"
      }
    },
    "radius": {
      "$value": "{border.radius.large}",
      "$type": "dimension"
    },
    "text": {
      "color": {
        "default": {
          "$value": "{penpot.color.accent.primary.default}",
          "$type": "color"
        },
        "inactive": {
          "$value": "{penpot.color.foreground.secondary}",
          "$type": "color"
        },
        "hover": {
          "$value": "{penpot.color.foreground.primary}",
          "$type": "color"
        },
        "onBackground": {
          "$value": "{grey.1000}",
          "$type": "color"
        }
      },
      "weight": {
        "default": {
          "$value": "{font.weight.accent}",
          "$type": "dimension"
        },
        "onBackground": {
          "$value": "{font.weight.normal}",
          "$type": "dimension"
        }
      },
      "size": {
        "$value": "{font.size.xsmall}",
        "$type": "dimension"
      },
      "letterSpacing": {
        "$value": "{font.letterSpacing.pos.xlarge}",
        "$type": "dimension"
      },
      "transform": {
        "$value": "uppercase",
        "$type": "string"
      }
    },
    "background": {
      "color": {
        "default": {
          "$value": "{penpot.color.accent.primary.muted}",
          "$type": "color"
        },
        "inactive": {
          "$value": "{penpot.color.background.tertiary}",
          "$type": "color"
        },
        "hover": {
          "$value": "{penpot.color.accent.tertiary}",
          "$type": "color"
        },
        "onBackground": {
          "$value": "rgba(255, 255, 255, 0.4)",
          "$type": "color"
        }
      }
    },
    "border": {
      "color": {
        "default": {
          "$value": "{grey.transparent}",
          "$type": "color"
        },
        "inactive": {
          "$value": "{grey.transparent}",
          "$type": "color"
        },
        "hover": {
          "$value": "{penpot.color.accent.tertiary}",
          "$type": "color"
        },
        "onBackground": {
          "$value": "{grey.transparent}",
          "$type": "color"
        }
      },
      "width": {
        "default": {
          "$value": "{size.null}",
          "$type": "dimension"
        },
        "inactive": {
          "$value": "{size.null}",
          "$type": "dimension"
        },
        "hover": {
          "$value": "{size.pos.unit}",
          "$type": "dimension"
        },
        "onBackground": {
          "$value": "{size.null}",
          "$type": "dimension"
        }
      },
      "offset": {
        "default": {
          "$value": "{size.null}",
          "$type": "dimension"
        },
        "inactive": {
          "$value": "{size.null}",
          "$type": "dimension"
        },
        "hover": {
          "$value": "{size.neg.unit}",
          "$type": "dimension"
        },
        "onBackground": {
          "$value": "{size.null}",
          "$type": "dimension"
        }
      }
    }
  }
}
