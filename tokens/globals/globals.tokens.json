{
  "$extensions": {
    "com.figma.modeName": "Default"
  },
  "grey": {
    "$type": "color",
    "$description": "A comprehensive set of grey color values to ensure consistent color usage across all supported platforms. These values can be used for backgrounds, borders, text, that must be preserved whatever the enabled color mode.",
    "$extensions": {
      "com.figma.scopes": ["ALL_SCOPES"]
    },
    "transparent": {
      "$value": {
        "colorSpace": "srgb",
        "components": [0, 0, 0],
        "alpha": 0,
        "hex": "#00000000"
      }
    },
    "0": {
      "$value": {
        "colorSpace": "srgb",
        "components": [1, 1, 1],
        "alpha": 1,
        "hex": "#FFFFFF"
      }
    },
    "100": {
      "$value": {
        "colorSpace": "srgb",
        "components": [0.941, 0.941, 0.941],
        "alpha": 1,
        "hex": "#F0F0F0"
      }
    },
    "200": {
      "$value": {
        "colorSpace": "srgb",
        "components": [0.851, 0.851, 0.851],
        "alpha": 1,
        "hex": "#D9D9D9"
      }
    },
    "300": {
      "$value": {
        "colorSpace": "srgb",
        "components": [0.749, 0.749, 0.749],
        "alpha": 1,
        "hex": "#BFBFBF"
      }
    },
    "400": {
      "$value": {
        "colorSpace": "srgb",
        "components": [0.651, 0.651, 0.651],
        "alpha": 1,
        "hex": "#A6A6A6"
      }
    },
    "500": {
      "$value": {
        "colorSpace": "srgb",
        "components": [0.549, 0.549, 0.549],
        "alpha": 1,
        "hex": "#8C8C8C"
      }
    },
    "600": {
      "$value": {
        "colorSpace": "srgb",
        "components": [0.451, 0.451, 0.451],
        "alpha": 1,
        "hex": "#737373"
      }
    },
    "700": {
      "$value": {
        "colorSpace": "srgb",
        "components": [0.349, 0.349, 0.349],
        "alpha": 1,
        "hex": "#595959"
      }
    },
    "800": {
      "$value": {
        "colorSpace": "srgb",
        "components": [0.251, 0.251, 0.251],
        "alpha": 1,
        "hex": "#404040"
      }
    },
    "900": {
      "$value": {
        "colorSpace": "srgb",
        "components": [0.173, 0.173, 0.173],
        "alpha": 1,
        "hex": "#2C2C2C"
      }
    },
    "1000": {
      "$value": {
        "colorSpace": "srgb",
        "components": [0.133, 0.133, 0.133],
        "alpha": 1,
        "hex": "#222222"
      }
    }
  },
  "border": {
    "radius": {
      "$type": "dimension",
      "$description": "A comprehensive set of radius values to ensure roundness across all supported platforms. These values can be used for border-radius, corner-radius, and other roundness needs.",
      "$extensions": {
        "com.figma.scopes": ["CORNER_RADIUS"]
      },
      "null": {
        "$value": { "value": 0, "unit": "px" }
      },
      "xsmall": {
        "$value": { "value": 1, "unit": "px" }
      },
      "small": {
        "$value": { "value": 2, "unit": "px" }
      },
      "medium": {
        "$value": { "value": 4, "unit": "px" }
      },
      "large": {
        "$value": { "value": 6, "unit": "px" }
      },
      "xlarge": {
        "$value": { "value": 8, "unit": "px" }
      },
      "xxlarge": {
        "$value": { "value": 12, "unit": "px" }
      },
      "xxxlarge": {
        "$value": { "value": 16, "unit": "px" }
      },
      "full": {
        "$value": { "value": 100, "unit": "px" }
      }
    }
  },
  "size": {
    "$type": "dimension",
    "$description": "A comprehensive set of spacing/sizing values to ensure consistent layout and spacing across all supported platforms. These values can be used for margins, paddings, gaps, and other spacing needs.",
    "$extensions": {
      "com.figma.scopes": [
        "WIDTH_HEIGHT",
        "GAP",
        "STROKE_FLOAT",
        "EFFECT_FLOAT",
        "OPACITY"
      ]
    },
    "neg": {
      "xxxlarge": {
        "$value": { "value": -80, "unit": "px" }
      },
      "xxlarge": {
        "$value": { "value": -64, "unit": "px" }
      },
      "xlarge": {
