      "xlarge": {
        "$value": { "value": 48, "unit": "px" }
      },
      "xxlarge": {
        "$value": { "value": 64, "unit": "px" }
      },
      "xxxlarge": {
        "$value": { "value": 80, "unit": "px" }
      }
    }
  },
  "font": {
    "$type": "typography",
    "$description": "A comprehensive set of typography values to ensure consistent text styles across all supported platforms. These values can be used for font sizes, weights, line heights, and letter spacing.",
    "size": {
      "$extensions": {
        "com.figma.scopes": ["FONT_SIZE"]
      },
      "xxxsmall": {
        "$type": "dimension",
        "$value": { "value": 9, "unit": "px" }
      },
      "xxsmall": {
        "$type": "dimension",
        "$value": { "value": 10, "unit": "px" }
      },
      "xsmall": {
        "$type": "dimension",
        "$value": { "value": 11, "unit": "px" }
      },
      "small": {
        "$type": "dimension",
        "$value": { "value": 12, "unit": "px" }
      },
      "medium": {
        "$type": "dimension",
        "$value": { "value": 13, "unit": "px" }
      },
      "large": {
        "$type": "dimension",
        "$value": { "value": 14, "unit": "px" }
      }
    },
    "weight": {
      "$extensions": {
        "com.figma.scopes": ["FONT_STYLE"]
      },
      "normal": {
        "$type": "number",
        "$value": 400
      },
      "accent": {
        "$type": "number",
        "$value": 450
      },
      "medium": {
        "$type": "number",
        "$value": 500
      },
      "semi": {
        "$type": "number",
        "$value": 550
      },
      "bold": {
        "$type": "number",
        "$value": 600
      }
    },
    "lineHeight": {
      "$extensions": {
        "com.figma.scopes": ["LINE_HEIGHT"]
      },
      "default": {
        "$type": "dimension",
        "$value": { "value": 16, "unit": "px" }
      },
      "large": {
        "$type": "dimension",
        "$value": { "value": 20, "unit": "px" }
      }
    },
    "letterSpacing": {
      "$extensions": {
        "com.figma.scopes": ["LETTER_SPACING"]
      },
      "neg": {
        "xlarge": {
          "$type": "dimension",
          "$value": { "value": -0.16, "unit": "px" }
        },
        "large": {
