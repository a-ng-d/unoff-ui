{
  "name": "@unoff/ui",
  "version": "1.21.1",
  "type": "module",
  "description": "Unoff UI is a comprehensive library of UI components designed specifically for building Figma and Penpot plugins. It leverages modern tools and frameworks to ensure a seamless development experience.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/a-ng-d/unoff-ui.git"
  },
  "keywords": [
    "library",
    "ui",
    "figma-ui"
  ],
  "author": "Aur√©lien Grimaud",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/a-ng-d/unoff-ui/issues"
  },
  "homepage": "https://github.com/a-ng-d/unoff-ui#readme",
  "overrides": {
    "storybook": "$storybook",
    "vite": "$vite"
  },
  "files": [
    "dist"
  ],
  "exports": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "start": "npm run storybook",
    "build": "tsc && vite build",
    "test:storybook": "vitest --config=vitest.config.ts",
    "test:coverage": "vitest --config=vitest.config.ts --coverage",
    "test:coverage:ui": "vitest --config=vitest.config.ts --coverage --ui",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "lint:css": "stylelint \"**/*.{css,scss}\"",
    "lint:css:fix": "stylelint \"**/*.{css,scss}\" --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "create:theme": "node scripts/create-theme.js",
    "scss:list": "node scripts/build-scss.js",
    "scss:build": "node scripts/build-scss.js -- --build",
    "figma:publish": "figma connect publish",
    "figma:unpublish": "figma connect unpublish",
    "figma:parse": "figma connect parse"
  },
  "dependencies": {
    "@unoff/utils": "^0.8.1"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^5.0.0",
    "@figma/code-connect": "^1.3.12",
    "@storybook/addon-a11y": "^10.1.11",
    "@storybook/addon-docs": "^10.1.11",
    "@storybook/addon-links": "^10.1.11",
    "@storybook/addon-mcp": "^0.1.8",
    "@storybook/addon-onboarding": "^10.1.11",
    "@storybook/addon-vitest": "^10.1.11",
    "@storybook/react-vite": "^10.1.11",
    "@terrazzo/cli": "^0.10.5",
    "@terrazzo/plugin-css": "^0.10.4",
    "@terrazzo/token-tools": "^0.10.5",
    "@testing-library/react": "^15.0.7",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/browser": "^4.0.17",
    "@vitest/browser-playwright": "^4.0.17",
    "@vitest/coverage-v8": "^4.0.17",
    "@vitest/ui": "^4.0.17",
    "chalk": "^5.6.2",
    "chromatic": "^11.3.1",
    "dotenv-cli": "^7.4.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "eslint-plugin-storybook": "^10.1.11",
    "glob": "^10.3.12",
    "jsdom": "^24.0.0",
    "playwright": "^1.57.0",
    "prettier": "^3.2.5",
    "prettier-plugin-css-order": "^2.1.2",
    "sass": "^1.77.0",
    "storybook": "^10.1.11",
    "stylelint": "^16.24.0",
    "stylelint-config-standard": "^39.0.0",
    "typescript": "^5.4.3",
    "vite": "^7.3.1",
    "vite-plugin-dts": "^3.9.1",
    "vite-plugin-lib-inject-css": "^2.0.1",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^4.0.17"
  }
}